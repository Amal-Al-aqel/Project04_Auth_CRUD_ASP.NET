@{
}

@model List<Profile>
@{
    ViewData["Title"] = "View User Claims";
}
<h1>@ViewData["Title"]</h1>
<br />
@if (User.Identity.IsAuthenticated)
{
    <table class="table table-responsive-sm">
        @foreach (var claim in User.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
                <td>@claim.Issuer</td>
            </tr>
        }
    </table>
}